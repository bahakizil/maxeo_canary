# Maxeo Canary Test Configuration

# Database Connection (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=maxeo-db
POSTGRES_USER=maxeo-user
POSTGRES_PASSWORD=your-password-here

# Canary Test Settings
CANARY_BASE_URL=https://maxeo.ai
CANARY_HEADLESS=true
CANARY_CATEGORY_WAIT_TIMEOUT=120
CANARY_SNAPSHOT_WAIT_TIMEOUT=300
CANARY_AUTO_CLEANUP=true
CANARY_CLEANUP_AFTER_HOURS=24

# Slack Alerting (Optional)
CANARY_SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Admin Credentials (for OTP generation)
ADMIN_TOTP_SECRET=your-32-char-totp-secret-here
FERNET_ENCRYPTION_KEY=your-fernet-key-here

# AI API Keys (for DB verification queries)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Development/Debug Options
CANARY_SKIP_OTP=false
CANARY_DEBUG=false

# Sentry (Optional - for error tracking)
SENTRY_DSN=
SENTRY_ENABLED=false
